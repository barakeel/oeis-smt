(assert (forall ((x Int)) (= (fast x) (+ (+ 2 (+ (* 2 (divf x (+ 2 2))) x)) (divf (+ 1 x) (+ 2 2))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
