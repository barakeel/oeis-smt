;; sequence(s): A10020
;; terms: 1 33 126 281 498 777 1118 1521 1986 2513 3102 3753 4466 5241 6078 6977 7938 8961 10046 11193
;; small program: loop(2 + (2 * (y * y)), 2 * (x + x), 1) - (x * x)
;; fast program: (((2 * (2 * (2 * (2 + 2)))) - 1) * (x * x)) + (if x <= 0 then 1 else 2)
(set-logic UFNIA)
(declare-fun f0 (Int Int) Int)
(declare-fun g0 (Int) Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 (Int) Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int) (y Int)) (= (f0 x y) (+ 2 (* 2 (* y y))))))
(assert (forall ((x Int)) (= (g0 x) (* 2 (+ x x)))))
(assert (= h0 1))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y) x)))))
(assert (forall ((x Int)) (= (v0 x) (u0 (g0 x) h0))))
(assert (forall ((x Int)) (= (small x) (- (v0 x) (* x x)))))
(assert (forall ((x Int)) (= (fast x) (+ (* (- (* 2 (* 2 (* 2 (+ 2 2)))) 1) (* x x)) (ite (<= x 0) 1 2)))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
