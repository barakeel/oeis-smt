(assert (forall ((x Int)) (= (fast x) (+ (- (* 2 (* 2 (+ 2 (* 2 (+ x x))))) 1) x))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
