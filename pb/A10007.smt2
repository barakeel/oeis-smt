;; sequence(s): A10007
;; terms: 1 19 70 155 274 427 614 835 1090 1379 1702 2059 2450 2875 3334 3827 4354 4915 5510 6139
;; small program: (x * x) + loop(2 + (y * y), 2 * (x + x), 1)
;; fast program: ((1 + (2 * (2 * (2 + 2)))) * (x * x)) + (if x <= 0 then 1 else 2)
(set-logic UFNIA)
(declare-fun f0 (Int Int) Int)
(declare-fun g0 (Int) Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 (Int) Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int) (y Int)) (= (f0 x y) (+ 2 (* y y)))))
(assert (forall ((x Int)) (= (g0 x) (* 2 (+ x x)))))
(assert (= h0 1))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y) x)))))
(assert (forall ((x Int)) (= (v0 x) (u0 (g0 x) h0))))
(assert (forall ((x Int)) (= (small x) (+ (* x x) (v0 x)))))
(assert (forall ((x Int)) (= (fast x) (+ (* (+ 1 (* 2 (* 2 (+ 2 2)))) (* x x)) (ite (<= x 0) 1 2)))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
