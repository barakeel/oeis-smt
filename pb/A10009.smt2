;; sequence(s): A10009
;; terms: 1 21 78 173 306 477 686 933 1218 1541 1902 2301 2738 3213 3726 4277 4866 5493 6158 6861
;; small program: (x * x) + loop(2 + ((y + y) * y), (x + x) + x, 1)
;; fast program: ((1 + (2 + (2 * (2 * (2 + 2))))) * (x * x)) + (if x <= 0 then 1 else 2)
(set-logic UFNIA)
(declare-fun f0 (Int Int) Int)
(declare-fun g0 (Int) Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 (Int) Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int) (y Int)) (= (f0 x y) (+ 2 (* (+ y y) y)))))
(assert (forall ((x Int)) (= (g0 x) (+ (+ x x) x))))
(assert (= h0 1))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y) x)))))
(assert (forall ((x Int)) (= (v0 x) (u0 (g0 x) h0))))
(assert (forall ((x Int)) (= (small x) (+ (* x x) (v0 x)))))
(assert (forall ((x Int)) (= (fast x) (+ (* (+ 1 (+ 2 (* 2 (* 2 (+ 2 2))))) (* x x)) (ite (<= x 0) 1 2)))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
