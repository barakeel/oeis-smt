;; sequence(s): A241850
;; terms: 20 21 24 29 36 45 56 69 84 101 120 141 164 189 216 245 276 309 344 381
;; small program: (x * x) + loop(2 * (2 + x), 2, 2)
;; fast program: (2 * (2 + (2 * (2 + 2)))) + (x * x)
(set-logic UFNIA)
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (* 2 (+ 2 x)))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (+ (* x x) v0))))
(assert (forall ((x Int)) (= (fast x) (+ (* 2 (+ 2 (* 2 (+ 2 2)))) (* x x)))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
