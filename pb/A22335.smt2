;; small program: loop((((y + y) + y) div (((1 + 2) + 2) + 2)) + x, x, 1) + x
;; fast program: 1 + ((((1 + 2) * ((x * x) - x)) div (2 + (2 * (2 + (2 + 2))))) + x)
(set-logic UFNIA)
(define-fun divf ((a Int) (b Int)) Int (ite (< 0 b) (div a b) (div (- a) (- b))))
(declare-fun f0 (Int Int) Int)
(declare-fun g0 (Int) Int)
(declare-fun h0 () Int)
