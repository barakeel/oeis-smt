;; sequence(s): A195315
;; terms: 1 33 97 193 321 481 673 897 1153 1441 1761 2113 2497 2913 3361 3841 4353 4897 5473 6081
;; small program: 1 + ((1 + x) * (loop(x * x, 2, 2) * x))
;; fast program: 1 + (2 * (2 * (2 * (2 * ((x * x) + x)))))
(set-logic UFNIA)
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (* x x))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (+ 1 (* (+ 1 x) (* v0 x))))))
(assert (forall ((x Int)) (= (fast x) (+ 1 (* 2 (* 2 (* 2 (* 2 (+ (* x x) x)))))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
