;; sequence(s): A241848
;; terms: 18 19 22 27 34 43 54 67 82 99 118 139 162 187 214 243 274 307 342 379
;; small program: 2 + ((x * x) + loop(x * x, 2, 2))
;; fast program: 2 + ((2 * (2 * (2 + 2))) + (x * x))
(set-logic UFNIA)
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (* x x))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (+ 2 (+ (* x x) v0)))))
(assert (forall ((x Int)) (= (fast x) (+ 2 (+ (* 2 (* 2 (+ 2 2))) (* x x))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
