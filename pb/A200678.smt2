;; small program: loop(loop(x + x, (y div 2) div 2, 1) + x, x, 1)
;; fast program: loop2(2 + (x + y), x - y, x div 2, 1 + (x mod 2), 0)
(set-logic UFNIA)
(define-fun divf ((a Int) (b Int)) Int (ite (< 0 b) (div a b) (div (- a) (- b))))
(define-fun modf ((a Int) (b Int)) Int (ite (< 0 b) (mod a b) (- (mod (- a) (- b)))))
