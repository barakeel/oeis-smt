(assert (forall ((x Int) (y Int)) (= (f0 x y) (+ 1 y))))
(assert (forall ((x Int)) (= (g0 x) (+ (+ (modf 2 x) x) x))))
(assert (forall ((x Int)) (= (h0 x) x)))
