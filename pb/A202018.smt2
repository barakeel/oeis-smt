;; sequence(s): A202018
;; terms: 41 43 47 53 61 71 83 97 113 131 151 173 197 223 251 281 313 347 383 421
;; small program: (((x * x) - 1) + loop((x * x) + x, 2, 2)) + x
;; fast program: 1 + (((x * x) + (2 * (2 * (2 + (2 * (2 + 2)))))) + x)
(set-logic UFNIA)
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (+ (* x x) x))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (+ (+ (- (* x x) 1) v0) x))))
(assert (forall ((x Int)) (= (fast x) (+ 1 (+ (+ (* x x) (* 2 (* 2 (+ 2 (* 2 (+ 2 2)))))) x)))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
