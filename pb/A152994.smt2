;; sequence(s): A152994
;; terms: 0 9 54 135 252 405 594 819 1080 1377 1710 2079 2484 2925 3402 3915 4464 5049 5670 6327
;; small program: ((x - 1) + x) * loop((x + x) + x, 2, x)
;; fast program: (1 + 2) * ((1 + 2) * ((2 * (x * x)) - x))
(set-logic UFNIA)
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 (Int) Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 (Int) Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (+ (+ x x) x))))
(assert (= g0 2))
(assert (forall ((x Int)) (= (h0 x) x)))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (forall ((x Int)) (= (v0 x) (u0 g0 (h0 x)))))
(assert (forall ((x Int)) (= (small x) (* (+ (- x 1) x) (v0 x)))))
(assert (forall ((x Int)) (= (fast x) (* (+ 1 2) (* (+ 1 2) (- (* 2 (* x x)) x))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
