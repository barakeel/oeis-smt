;; sequence(s): A70443
;; terms: 0 1 4 9 16 4 15 7 1 18 16 16 18 1 7 15 4 16 9 4
;; small program: (x * x) mod (1 + loop(2 * (2 + x), 2, 2))
;; fast program: (x * x) mod (1 + (2 * (2 + (2 * (2 + 2)))))
(set-logic UFNIA)
(define-fun modf ((a Int) (b Int)) Int (ite (< 0 b) (mod a b) (- (mod (- a) (- b)))))
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (* 2 (+ 2 x)))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (modf (* x x) (+ 1 v0)))))
(assert (forall ((x Int)) (= (fast x) (modf (* x x) (+ 1 (* 2 (+ 2 (* 2 (+ 2 2)))))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
