;; sequence(s): A110678
;; terms: 72 70 66 60 52 42 30 16 0 ~18 ~38 ~60 ~84 ~110 ~138 ~168 ~200 ~234 ~270 ~308
;; small program: (loop(2 * ((x + x) + x), 2, 2) - (x * x)) - x
;; fast program: (1 + ((2 * (2 + 2)) + x)) * ((2 * (2 + 2)) - x)
(set-logic UFNIA)
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (* 2 (+ (+ x x) x)))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (- (- v0 (* x x)) x))))
(assert (forall ((x Int)) (= (fast x) (* (+ 1 (+ (* 2 (+ 2 2)) x)) (- (* 2 (+ 2 2)) x)))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
