(assert (forall ((x Int)) (= (fast x) (divf (* (+ 1 x) (- (* 2 (* 2 (* (+ 1 2) (+ 1 x)))) x)) 2))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
