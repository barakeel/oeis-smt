;; sequence(s): A114444
;; terms: 0 48 128 240 384 560 768 1008 1280 1584 1920 2288 2688 3120 3584 4080 4608 5168 5760 6384
;; small program: (2 + x) * (loop(x * x, 2, 2) * x)
;; fast program: 2 * (2 * (2 * (2 * ((2 + x) * x))))
(set-logic UFNIA)
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (* x x))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (* (+ 2 x) (* v0 x)))))
(assert (forall ((x Int)) (= (fast x) (* 2 (* 2 (* 2 (* 2 (* (+ 2 x) x))))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
