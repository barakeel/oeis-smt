;; sequence(s): A61824
;; terms: 0 8 16 24 32 40 48 56 64 72 80 88 104 112 120 128 136 144 152 160
;; small program: (((x + x) div loop(1 + (2 * (2 + x)), 2, 2)) + x) * (2 * (2 + 2))
;; fast program: (((x + x) div ((2 * (2 * (2 + (2 + 2)))) - 1)) + x) * (2 * (2 + 2))
(set-logic UFNIA)
(define-fun divf ((a Int) (b Int)) Int (ite (< 0 b) (div a b) (div (- a) (- b))))
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (+ 1 (* 2 (+ 2 x))))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (* (+ (divf (+ x x) v0) x) (* 2 (+ 2 2))))))
(assert (forall ((x Int)) (= (fast x) (* (+ (divf (+ x x) (- (* 2 (* 2 (+ 2 (+ 2 2)))) 1)) x) (* 2 (+ 2 2))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
