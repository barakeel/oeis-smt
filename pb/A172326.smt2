;; sequence(s): A172326
;; terms: 0 4 8 12 16 20 24 28 32 36 40 44 48 52 56 60 64 69 73 77
;; small program: (2 * (x + x)) + (x div (1 + loop(x * x, 2, 2)))
;; fast program: (2 * (x + x)) + (x div (1 + (2 * (2 * (2 + 2)))))
(set-logic UFNIA)
(define-fun divf ((a Int) (b Int)) Int (ite (< 0 b) (div a b) (div (- a) (- b))))
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int)) (= (f0 x) (* x x))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y))))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (+ (* 2 (+ x x)) (divf x (+ 1 v0))))))
(assert (forall ((x Int)) (= (fast x) (+ (* 2 (+ x x)) (divf x (+ 1 (* 2 (* 2 (+ 2 2)))))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
