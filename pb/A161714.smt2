;; sequence(s): A161714
;; terms: 1 29 57 85 113 141 169 197 225 253 281 309 337 365 393 421 449 477 505 533
;; small program: 1 + ((loop((x * x) + y, 2, 2) * x) + x)
;; fast program: 1 + (2 * (2 * ((2 * ((x + x) + x)) + x)))
(set-logic UFNIA)
(declare-fun f0 (Int Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int) (y Int)) (= (f0 x y) (+ (* x x) y))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y) x)))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (+ 1 (+ (* v0 x) x)))))
(assert (forall ((x Int)) (= (fast x) (+ 1 (* 2 (* 2 (+ (* 2 (+ (+ x x) x)) x)))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
