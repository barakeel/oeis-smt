;; sequence(s): A145583
;; terms: 49 16 6 3
;; small program: (1 + loop((x + y) * x, 2, 2)) div (if x <= 0 then 1 else ((2 + x) * x))
;; fast program: (1 + (2 * (2 * (2 * (2 + (2 + 2)))))) div (if x <= 0 then 1 else ((2 + x) * x))
(set-logic UFNIA)
(define-fun divf ((a Int) (b Int)) Int (ite (< 0 b) (div a b) (div (- a) (- b))))
(declare-fun f0 (Int Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 () Int)
(declare-fun u0 (Int Int) Int)
(declare-fun v0 () Int)
(declare-fun small (Int) Int)
(declare-fun fast (Int) Int)
(assert (forall ((x Int) (y Int)) (= (f0 x y) (* (+ x y) x))))
(assert (= g0 2))
(assert (= h0 2))
(assert (forall ((x Int) (y Int)) (= (u0 x y) (ite (<= x 0) y (f0 (u0 (- x 1) y) x)))))
(assert (= v0 (u0 g0 h0)))
(assert (forall ((x Int)) (= (small x) (divf (+ 1 v0) (ite (<= x 0) 1 (* (+ 2 x) x))))))
(assert (forall ((x Int)) (= (fast x) (divf (+ 1 (* 2 (* 2 (* 2 (+ 2 (+ 2 2)))))) (ite (<= x 0) 1 (* (+ 2 x) x))))))
(assert (exists ((c Int)) (and (>= c 0) (not (= (small c) (fast c))))))
(check-sat)
