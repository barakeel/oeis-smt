;; small program: ((2 + x) mod (1 + 2)) + loop(1 + (x + x), 2, x)
;; fast program: 1 + (2 + ((2 * (x + x)) + ((2 + x) mod (1 + 2))))
(set-logic UFNIA)
(define-fun modf ((a Int) (b Int)) Int (ite (< 0 b) (mod a b) (- (mod (- a) (- b)))))
(declare-fun f0 (Int) Int)
(declare-fun g0 () Int)
(declare-fun h0 (Int) Int)
